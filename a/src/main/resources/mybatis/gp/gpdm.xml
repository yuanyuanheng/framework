<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gpdm">
	<insert id="save" parameterType="Map">
		insert into gp_gp(gpdm,gpmc,zt) values(#{gpdm},#{gpmc},#{zt})
	</insert>
	<select id="select" parameterType="int" resultType="Map">
		select * from gp_gpdm where gpdm = #{gpdm}
	</select>
	<update id="update" parameterType="Map">
		update gp_gpdm set gpdm = #{gpdm},gpmc = #{gpmc},zt = #{zt} where gpdm = #{gpdm}
	</update>
</mapper>