<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gpdm">
	<insert id="save" parameterType="Gpdm">
		replace into gp_gpdm(gpdm,gpmc,zt) values(#{gpdm},#{gpmc},#{zt}) 
	</insert>
	
	<select id="selectObj" parameterType="String" resultType="Gpdm">
		select * from gp_gpdm where gpdm = #{gpdm}
	</select>
	<select id="selectList" parameterType="Gpdm" resultType="List">
		select * from gp_gpdm 
	</select>
	<update id="update" parameterType="Gpdm">
		update gp_gpdm set gpdm = #{gpdm},gpmc = #{gpmc},zt = #{zt} where gpdm = #{gpdm}
	</update>
</mapper>